//JAVA program to display fibonacci series of n numbers
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	     int n1=0,n2=1,n,fib=0,i;
	     System.out.println("enter the number upto which fibonacci series is requried: ");
	     Scanner in=new Scanner(System.in);
	     n=in.nextInt();
	      System.out.println("Fibonacci series of "+ n +"numbers is ");
	       
	     for(i=1;i<=n;++i)
	     {
	         System.out.print(n1 + ", ");
             fib=n1+n2;
	         n1=n2;
	         n2=fib;
	     }
	     
	 }
}
